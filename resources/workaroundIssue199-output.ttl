@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix fno: <https://w3id.org/function/ontology#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix ex: <http://mapping.example.com/>.
@prefix idlabfn: <http://example.com/idlab/function/>.
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#>.
@prefix grel: <http://users.ugent.be/~bjdmeest/function/grel.ttl#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.

ex:rules_000 a <http://rdfs.org/ns/void#Dataset>;
    <http://rdfs.org/ns/void#exampleResource> ex:map_stops_000.
ex:map_stops_000 a rr:TriplesMap;
    rml:logicalSource ex:source_000;
    rdfs:label "stops";
    rr:subjectMap ex:s_000;
    rr:predicateObjectMap ex:pom_000, ex:pom_001.
ex:source_000 a rml:LogicalSource;
    rml:source "./data/gtfs-csv-01/STOPS.csv";
    rml:referenceFormulation ql:CSV.
ex:s_000 a rr:SubjectMap;
    rr:template "http://transport.linkeddata.es/madrid/metro/stops/{stop_id}".
ex:pom_000 a rr:PredicateObjectMap;
    rr:predicateMap ex:pm_000;
    rr:objectMap ex:om_000.
ex:pm_000 a rr:PredicateMap;
    rr:constant rdf:type.
ex:om_000 a rr:ObjectMap;
    rr:constant "http://vocab.gtfs.org/terms#Stop";
    rr:termType rr:IRI.
ex:pom_001 a rr:PredicateObjectMap;
    rr:predicateMap ex:pm_001;
    rr:objectMap ex:om_001.
ex:pm_001 a rr:PredicateMap;
    rr:constant <http://vocab.gtfs.org/terms#parentStation>.
ex:om_001 a rr:ObjectMap;
    rr:parentTriplesMap ex:map_stops_000_issue;
    rr:joinCondition ex:jc_000.
ex:jc_000 rr:child "parent_station";
    rr:parent "stop_id".
ex:map_stops_000_issue a rr:TriplesMap;
    rml:logicalSource ex:source_000;
    rr:subjectMap ex:s_000.
