@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#>.
@prefix schema: <http://schema.org/>.

gtfs:Agency a sh:NodeShape;
    sh:targetClass gtfs:Agency;
    sh:property gtfs:timeZone-shape, dct:language-shape, foaf:page-shape, foaf:phone-shape, foaf:name-shape, gtfs:fareUrl-shape.
gtfs:timeZone-shape a sh:PropertyShape;
    sh:path gtfs:timeZone.
dct:language-shape a sh:PropertyShape;
    sh:path dct:language.
foaf:page-shape a sh:PropertyShape;
    sh:path foaf:page.
foaf:phone-shape a sh:PropertyShape;
    sh:path foaf:phone.
foaf:name-shape a sh:PropertyShape;
    sh:path foaf:name.
gtfs:fareUrl-shape a sh:PropertyShape;
    sh:path gtfs:fareUrl.
gtfs:CalendarDateRule a sh:NodeShape;
    sh:targetClass gtfs:CalendarDateRule;
    sh:property gtfs:dateAddition-shape, dct:date-shape.
gtfs:dateAddition-shape a sh:PropertyShape;
    sh:path gtfs:dateAddition;
    sh:datatype xsd:boolean.
dct:date-shape a sh:PropertyShape;
    sh:path dct:date;
    sh:datatype xsd:date.
gtfs:CalendarRule a sh:NodeShape;
    sh:targetClass gtfs:CalendarRule;
    sh:property gtfs:saturday-shape, gtfs:tuesday-shape, gtfs:sunday-shape, gtfs:friday-shape, schema:endDate-shape, gtfs:monday-shape, gtfs:thursday-shape, schema:startDate-shape, gtfs:wednesday-shape.
gtfs:saturday-shape a sh:PropertyShape;
    sh:path gtfs:saturday;
    sh:datatype xsd:boolean.
gtfs:tuesday-shape a sh:PropertyShape;
    sh:path gtfs:tuesday;
    sh:datatype xsd:boolean.
gtfs:sunday-shape a sh:PropertyShape;
    sh:path gtfs:sunday;
    sh:datatype xsd:boolean.
gtfs:friday-shape a sh:PropertyShape;
    sh:path gtfs:friday;
    sh:datatype xsd:boolean.
schema:endDate-shape a sh:PropertyShape;
    sh:path schema:endDate;
    sh:datatype xsd:date.
gtfs:monday-shape a sh:PropertyShape;
    sh:path gtfs:monday;
    sh:datatype xsd:boolean.
gtfs:thursday-shape a sh:PropertyShape;
    sh:path gtfs:thursday;
    sh:datatype xsd:boolean.
schema:startDate-shape a sh:PropertyShape;
    sh:path schema:startDate;
    sh:datatype xsd:date.
gtfs:wednesday-shape a sh:PropertyShape;
    sh:path gtfs:wednesday;
    sh:datatype xsd:boolean.
gtfs:Feed a sh:NodeShape;
    sh:targetClass gtfs:Feed;
    sh:property dct:language-shape, foaf:page-shape, schema:endDate-shape, schema:startDate-shape, schema:version-shape, dct:publisher-shape.
schema:version-shape a sh:PropertyShape;
    sh:path schema:version.
dct:publisher-shape a sh:PropertyShape;
    sh:path dct:publisher.
gtfs:Frequency a sh:NodeShape;
    sh:targetClass gtfs:Frequency;
    sh:property gtfs:startTime-shape, gtfs:endTime-shape, gtfs:headwaySeconds-shape, gtfs:trip-shape, gtfs:exactTimes-shape.
gtfs:startTime-shape a sh:PropertyShape;
    sh:path gtfs:startTime.
gtfs:endTime-shape a sh:PropertyShape;
    sh:path gtfs:endTime.
gtfs:headwaySeconds-shape a sh:PropertyShape;
    sh:path gtfs:headwaySeconds;
    sh:datatype xsd:integer.
gtfs:trip-shape a sh:PropertyShape;
    sh:path gtfs:trip.
gtfs:exactTimes-shape a sh:PropertyShape;
    sh:path gtfs:exactTimes;
    sh:datatype xsd:boolean.
gtfs:Route a sh:NodeShape;
    sh:targetClass gtfs:Route;
    sh:property gtfs:longName-shape, gtfs:routeType-shape, gtfs:color-shape, gtfs:textColor-shape, gtfs:agency-shape, gtfs:shortName-shape, dct:description-shape, gtfs:routeUrl-shape.
gtfs:longName-shape a sh:PropertyShape;
    sh:path gtfs:longName.
gtfs:routeType-shape a sh:PropertyShape;
    sh:path gtfs:routeType.
gtfs:color-shape a sh:PropertyShape;
    sh:path gtfs:color.
gtfs:textColor-shape a sh:PropertyShape;
    sh:path gtfs:textColor.
gtfs:agency-shape a sh:PropertyShape;
    sh:path gtfs:agency.
gtfs:shortName-shape a sh:PropertyShape;
    sh:path gtfs:shortName.
dct:description-shape a sh:PropertyShape;
    sh:path dct:description.
gtfs:routeUrl-shape a sh:PropertyShape;
    sh:path gtfs:routeUrl.
gtfs:Service a sh:NodeShape;
    sh:targetClass gtfs:Service;
    sh:property gtfs:serviceRule-shape.
gtfs:serviceRule-shape a sh:PropertyShape;
    sh:path gtfs:serviceRule.
gtfs:ShapePoint a sh:NodeShape;
    sh:targetClass gtfs:ShapePoint;
    sh:property geo:long-shape, geo:lat-shape, gtfs:distanceTraveled-shape, gtfs:pointSequence-shape.
geo:long-shape a sh:PropertyShape;
    sh:path geo:long;
    sh:datatype xsd:double.
geo:lat-shape a sh:PropertyShape;
    sh:path geo:lat;
    sh:datatype xsd:double.
gtfs:distanceTraveled-shape a sh:PropertyShape;
    sh:path gtfs:distanceTraveled.
gtfs:pointSequence-shape a sh:PropertyShape;
    sh:path gtfs:pointSequence.
gtfs:Shape a sh:NodeShape;
    sh:targetClass gtfs:Shape;
    sh:property gtfs:shapePoint-shape.
gtfs:shapePoint-shape a sh:PropertyShape;
    sh:path gtfs:shapePoint.
gtfs:Stop a sh:NodeShape;
    sh:targetClass gtfs:Stop;
    sh:property gtfs:timeZone-shape, foaf:page-shape, foaf:name-shape, dct:description-shape, geo:long-shape, geo:lat-shape, gtfs:locationType-shape, gtfs:code-shape, gtfs:parentStation-shape, gtfs:wheelchairAccessible-shape, gtfs:zone-shape, dct:identifier-shape.
gtfs:locationType-shape a sh:PropertyShape;
    sh:path gtfs:locationType.
gtfs:code-shape a sh:PropertyShape;
    sh:path gtfs:code.
gtfs:parentStation-shape a sh:PropertyShape;
    sh:path gtfs:parentStation.
gtfs:wheelchairAccessible-shape a sh:PropertyShape;
    sh:path gtfs:wheelchairAccessible.
gtfs:zone-shape a sh:PropertyShape;
    sh:path gtfs:zone.
dct:identifier-shape a sh:PropertyShape;
    sh:path dct:identifier.
gtfs:StopTime a sh:NodeShape;
    sh:targetClass gtfs:StopTime;
    sh:property gtfs:trip-shape, gtfs:distanceTraveled-shape, gtfs:stop-shape, gtfs:arrivalTime-shape, gtfs:pickupType-shape, gtfs:departureTime-shape, gtfs:headsign-shape, gtfs:stopSequence-shape, gtfs:dropOffType-shape.
gtfs:stop-shape a sh:PropertyShape;
    sh:path gtfs:stop.
gtfs:arrivalTime-shape a sh:PropertyShape;
    sh:path gtfs:arrivalTime;
    sh:datatype xsd:duration.
gtfs:pickupType-shape a sh:PropertyShape;
    sh:path gtfs:pickupType.
gtfs:departureTime-shape a sh:PropertyShape;
    sh:path gtfs:departureTime;
    sh:datatype xsd:duration.
gtfs:headsign-shape a sh:PropertyShape;
    sh:path gtfs:headsign.
gtfs:stopSequence-shape a sh:PropertyShape;
    sh:path gtfs:stopSequence;
    sh:datatype xsd:integer.
gtfs:dropOffType-shape a sh:PropertyShape;
    sh:path gtfs:dropOffType.
gtfs:Trip a sh:NodeShape;
    sh:targetClass gtfs:Trip;
    sh:property gtfs:shortName-shape, gtfs:wheelchairAccessible-shape, gtfs:headsign-shape, gtfs:route-shape, gtfs:service-shape, gtfs:block-shape, gtfs:direction-shape, gtfs:shape-shape.
gtfs:route-shape a sh:PropertyShape;
    sh:path gtfs:route.
gtfs:service-shape a sh:PropertyShape;
    sh:path gtfs:service.
gtfs:block-shape a sh:PropertyShape;
    sh:path gtfs:block.
gtfs:direction-shape a sh:PropertyShape;
    sh:path gtfs:direction.
gtfs:shape-shape a sh:PropertyShape;
    sh:path gtfs:shape.

